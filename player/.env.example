# Backblaze B2 Configuration
B2_KEY_ID=your_b2_key_id_here
B2_KEY=your_b2_key_here
B2_BUCKET_NAME=your_bucket_name_here

# Player Settings
# Directory where downloaded sessions will be stored
DOWNLOAD_DIR=downloads

# Playback speed multiplier (1.0 = normal speed, 2.0 = 2x faster, 0.5 = 2x slower)
PLAYBACK_SPEED=1.0

# Auto-play when session is loaded
AUTO_PLAY=false

